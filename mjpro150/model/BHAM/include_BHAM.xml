                <!-- ======= THUMB  ======= -->
                <body name="thumb0">
               	<joint name="thumb_ABD" type="hinge" pos="0.0443196 0.0787516 0.0622977" axis="0.549586 -0.456218 0.699872" range="-0.26180 0.26180"/>
