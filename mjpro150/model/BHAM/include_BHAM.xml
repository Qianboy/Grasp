							</body>
						</body>
					</body>
				</body>
				
				<!-- ======= MIDDLE  ======= -->
                <body name="middle0">
					<joint name="middle_ABD" type="hinge" pos="0.0101112 0.117841 -0.00374828" axis="0.000000 0.000000 -1.000000" range="-0.26180 0.26180"/>
					<geom type="mesh" mesh="middle_basecover"/>
					<body name="middle1">
