				<!-- ======= LITTLE  ======= -->
                <body name="little0">
					<joint name="little_ABD" type="hinge" pos="-0.050761 0.07157 -0.016541" axis="-0.173648 0.000000 -0.984808" range="-0.26180 0.26180"/>
					<geom type="mesh" mesh="little_basecover"/>
					<body name="little1">
