					<body name="thumb1">
						<joint name="thumb_IN" type="hinge" pos="0.036754 0.056714 0.04474" axis="-0.814200 -0.104803 0.571047" range="0.087266 1.1345" ref="0.087266"/>
                		<geom type="mesh" mesh="thumb_proximalp"/>
						<body name="thumb2">
							<joint name="thumb_OUT" type="hinge" pos="0.046615 0.10503 0.06708" axis="-0.814200 -0.104803 0.571047" range="0.087266 1.4835" ref="0.087266"/>
